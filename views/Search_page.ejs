<html>
    <head>
        <title>Search_page</title>
        <% include ./partials/wiki_header %>
    </head>
    <style>
        .detail_frame {
            width: 100%;
            height: 100px;
            /* border: 1px solid red;    */
        }
        .detail_block_frame {
            width: 70%;
            height: 100%;
            margin: 0 15% 0;
            background-color: white;
            border: 0.5px solid rgb(144, 144, 146);   
        }
        .detail_title_frame{
            position: inline-block;
            width: 80%;
            height: 50;
            margin: 3.5% 5% auto;
        }
        .search_title {
            width: 100%;
            height: 100%;
            font-size: 2em;
            text-decoration:none;
            color: black;
        }
    </style>
    <body>
        <% for( var i = 0; i<rows.length; i++) { %>
        <div class="detail_frame">
            <div class="detail_block_frame">
                <div class="detail_title_frame">
                    <a href="/WikiHome/Detail/<%= rows[i].title %>" class="search_title"><%= rows[i].title %></a>
                </div>
            </div>
        </div>
        <% } %>
    </body>
</html>